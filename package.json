  "private": true,
  "scripts": {
    "vercel-build": "npm run pagefind && npm run next-build",
    "dev": "next dev --turbopack --port 4030",
    "next-build": "next build --no-lint",
    "start": "next start",
    "lint": "eslint && prettier --list-different .",
    "lint-fix": "eslint --fix && prettier --write .",
    "typecheck": "tsc --noEmit",
    "typecheck-watch": "tsc --noEmit --watch",
    "index": "blogkit-internal-tool index",
    "index-watch": "watchexec -w src/contents/ -w src/terms/ -- npm run index",
    "pagefind": "./scripts/pagefind.sh",
    "vercel-deploy-prod-from-local": "vercel build --prod && vercel deploy --prebuilt --prod"
  },
