    "@luma-dev/string-util-ts": "npm:@jsr/luma-dev__string-util-ts@^1.0.0",
    "@mdx-js/loader": "^3.1.0",
    "@mdx-js/react": "^3.1.0",
    "@next/mdx": "^15.3.3",
    "@types/mdx": "^2.0.13",
    "flip-toolkit": "^7.2.6",
    "inline-style-parser": "^0.2.4",
    "neverthrow": "^8.2.0",
    "next": "~15.3.3",
    "next-mdx-remote": "^5.0.0",
    "next-seo": "^6.8.0",
    "nuqs": "^2.4.3",
    "printf": "^0.6.1",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-flip-toolkit": "^7.2.4",
    "react-icons": "^5.5.0",
    "remark-breaks": "^4.0.0",
    "remark-frontmatter": "^5.0.0",
    "remark-gfm": "^4.0.1",
    "remark-math": "^6.0.0",
    "swr": "^2.3.3",
    "wolfram-notebook-embedder": "^0.3.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@types/node": "^22",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.3.3",
    "pagefind": "^1.3.0",
    "prettier": "^3.5.3",
    "shx": "^0.4.0",
    "typescript": "~5.8",
    "typescript-eslint": "^8.33.1",
    "vitest": "^3.2.2"
